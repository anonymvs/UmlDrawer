digraph G {
	fontname = "Bitstream Vera Sans"
	fontsize = 8


 	node [
		fontname = "Bitstream Vera Sans"
		fontsize = 8
		shape = "record"
	]

	edge [
		fontname = "Bitstream Vera Sans"
		fontsize = 8
	]

	Cat [
		label = <{Cat||}>
	]

	City [
